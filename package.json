{
  "name": "stream-from-iterator",
  "version": "1.0.0",
  "description": "Create streams from ES2015 iterators",
  "keywords": [
    "readable",
    "object",
    "stream",
    "adapter",
    "to stream",
    "es6",
    "es2015",
    "iterator",
    "generator"
  ],
  "repository": "schnittstabil/stream-from-iterator",
  "author": {
    "name": "Michael Mayer",
    "email": "michael@schnittstabil.de",
    "url": "schnittstabil.de"
  },
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "ava": "^0.16.0",
    "coveralls": "^2.11.12",
    "es6-iterator": "^2.0.0",
    "execa": "^0.4.0",
    "nyc": "^8.1.0",
    "rimraf": "^2.5.4",
    "stream-recorder": "^0.3.0",
    "xo": "^0.16.0"
  },
  "xo": {
    "overrides": [
      {
        "files": "test/*.js",
        "rules": {
          "ava/test-ended": "off",
          "no-extend-native": "off",
          "no-use-extend-native/no-use-extend-native": "off"
        }
      }
    ]
  },
  "scripts": {
    "clean": "rimraf .nyc_output/ coverage/",
    "test": "xo && ava",
    "test-cover": "xo && nyc --all ava",
    "test-cover-html": "npm run test-cover && nyc report --reporter=html"
  }
}
